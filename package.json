{
  "name": "create-bod-app",
  "private": true,
  "version": "3.5.2",
  "description": "Boilerplate CLI App",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build -ws --if-present",
    "build:badge": "node scripts/badge.js",
    "e2e": "npm run test:e2e",
    "format": "npm run format -ws --if-present",
    "lint": "npm run lint -ws --if-present",
    "postbuild": "npm run build:badge",
    "publish": "npm publish -ws",
    "prelease": "standard-version --dry-run -s",
    "release": "standard-version -s",
    "start": "npm run start:bod",
    "start:all": "npm start -ws --if-present",
    "start:bod": "npm start -w bod",
    "test": "npm run test:bod",
    "test:all": "npm run test:all -ws --if-present",
    "test:bod": "npm run test:watch -w bod",
    "test:e2e": "node scripts/e2e.js"
  },
  "keywords": [
    "boilerplate",
    "cli",
    "create-react-app",
    "react",
    "react-scripts",
    "starter",
    "starter-kit",
    "template",
    "typescript"
  ],
  "author": "sabertazimi <sabertazimi@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/bod.git"
  },
  "homepage": "https://github.com/sabertazimi/bod#readme",
  "bugs": {
    "url": "https://github.com/sabertazimi/bod/issues"
  },
  "devDependencies": {
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "node-fetch": "^2.6.1",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.1",
    "ts-node": "^10.2.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "verdaccio": "^5.1.2"
  }
}
